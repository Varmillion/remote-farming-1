rOverlay{\n\theight: 80vh;\n\twidth: 100vw;\n\tposition: absolute;\n\tbackground-color: black;\n\ttop: 0;\n\tleft: 0;\n\topacity: .5;\n}\n/* Overlay part ENDs! */\n\n\n/* The text Over the Video! */\n.headerContent{\n\t/*background-color: black;*/\n\topacity: .5;\n\tcolor: white;\n\tposition: absolute;\n\ttop: 5%;\n\tleft: 5%;\n\tz-index: 1;\n\tline-height: 5.5;\n\tfont-size: 2rem;\n\tmargin: 10%;\n\n}\n.vddiHeading{\n\tcolor: white;\n}\n.abc{\n\tcolor: white;\n}\n\n/* text on the video part ENDs! */\n\n\n\n/* The menu in the header! */\n\n.headerLogoMenu{\n\twidth: 100%;\n\theight: 10%;\n\tdisplay: flex;\n\tjustify-content: space-between;\n\tposition: absolute;\n\ttop: 0;\n\tmargin-left: 0;\n}\n\n.nav-toggle{\n\tmargin: 5%;\n\tdisplay: none;\n}\n.nav-toggle:checked ~ nav{\n\tdisplay: block;\n}\n\n.headerMenu{\n\tdisplay: none;\n\tposition: absolute;\n\tright: 10px;\n\ttop: 50px;\n}\n\n\nli{\n\tlist-style-type: none;\n}\nnav li{\n\tbackground-color: black;\n\tcolor: white;\n\n\tmargin top: 20px;\n\tpadding: 5px;\n}\nnav li:hover{\n\tcolor: white;\n\tbackground-color: black;\n}\n.nav-toggle-label{\n\tcolor: white;\n}\n\n/*.menuLinks{\n\tcolor: black;\n\tbackground-color: black;\n}*/\n\n/* The header menu ENDs */\n\n/* The logo part in the header! */\n.headerPNG{\n\tmin-width: 50px;\n\twidth: 80px;\n\tposition: absolute;\n\ttop: 0;\n\tleft:0;\n\tmargin: 10px;\n\tpadding-left: 20px;\n}\n\n\n/* HEADER PART ENDs here! */\n/* The three column portion with Modules description! */\n.modules-headings{\n\t\n}\n\n.topics-description{\n\tpadding: 4%;\n\ttext-align: center;\n}\n.about{\n\t\n\ttext-align: center;\n\tmargin: 10%;\n}\n.about-p{\n\tmargin: 2%;\n}\n.p1{\n\tfont-size: 2rem;\n\tline-height: 30px;\n}\n\n@media screen and (min-width: 718px){\n\n\n\t\n\t.modules-headings{\n\tdisplay: flex;\n\tjustify-content: center;\n\t}\n\n\t.topics-description{\n\t\tflex:1 100%;\n\t\tpadding: 3%;\n\t\ttext-align: center;\n\t}\n\t.about{\n\t\tdisplay: flex;\n\t\ttext-align: center;\n\t\talign-items: center;\n\t\tmargin: 10%;\n\t}\n\n\t.about-p{\n\t\twidth: 45%;\n\t\tmargin: 2%;\n\n\t}\n\t.p1{\n\t\tfont-size: 2rem;\n\t\tline-height: 30px;\n\t}\n\t.p2{\n\t\tfloat: right;\n\t}\n\t.headerLogoMenu{\n\twidth: 100%;\n\theight: 10%;\n\toverflow: hidden;\n\tdisplay: flex;\n\tjustify-content: space-between;\n\tposition: absolute;\n\ttop: 0;\n\tmargin-left: 0;\n\tpadding: 10px; \n\t}\n\t.nav-toggle{\n\t\tdisplay: none;\n\t}\n\t.headerMenu{\n\tdisplay: block;\n\tposition: absolute;\n\tright: 0;\n\ttop: 10px;\n\n}\n  .nav-toggle-label{\ndisplay: none;\n  }\n  nav{\n    all:unset;\n  }\n/*\n\n\t.headerMenu{\n\t\tdisplay: block;\n\t\tcolor: black;\n\t\tpadding: 10px;\n\n\n\t}*/\n\n\tli{\n\tlist-style-type: none;\n\t}\n\tnav li{\n\t\tcolor: white;\n\t\tmargin: 4px;\n\t\tdisplay: inline-block;\n\t\tfloat: left;\n\t\tpadding: 10px;\n\t}\n\tnav li:hover{\n\t\tcolor: white;\n\t\tbackground-color: black;\n\t}\n\t.menuLinks{\n\t\tcolor: black;\n\t\tbackground-color: black;\n\n\t}\n\n\n}\n\n\n/*\n@media screen and (min-width:768px){\n\t.modules-headings{\n\tdisplay: flex;\n\tjustify-content: left;\n}\n\n\n}*/\n\nol li{\n\tcolor: white;\n\ttext-align: left;\n\tbackground-color: black;\n\tpadding: 10px;\n\tmargin: 10px;\n}\nol li:hover{\n\tcolor: black;\n\tbackground-color: white;\n\ttransition-duration: .6s;\n}\nol a{\n\tdisplay: block;\n\tcolor: white;\n\ttext-align: left;\n\tbackground-color: black;\n\tpadding: 10px;\n\tmargin: 10px;\n}\nol a:hover{\n\tcolor: black;\n\tbackground-color: white;\n\ttransition-duration: .6s;\n}\n\nhr{\n\twidth: 50%;\n\n}\n\n\n\n\nfooter{\n\twidth: 100%;\n    height: 10%;\n    display: flex;\n  \tflex-direction:column;\n  \tjustify-content:flex-end;\n }\n\n.footerThanks{\n  display:flex;\n  justify-content:flex-end;\n    font-size: 1rem;\n    margin: 1%;\n}\n\n.social{\n   display:flex;\n  justify-content:flex-end;\n  margin: 1%;\n  }\n\n\n\n\n"},"lineCount":323}},"error":null,"hash":"732859e76b7ee78f7b911dbad498b53f","cacheData":{"env":{}